  <div class="orderupdate-container py-5 text-center">
    <p class="text-muted">Customer:</p>
        <div class="form-group mb-4">
          <label class="text-muted" for="estimatedTime">Enter Estimated Time:</label>
          <input class="form-control mb-3" type="number" name="estimatedTime" placeholder="How long will it take?">
          <button type="submit" class="btn btn-primary">Submit</button>
        </div>
      </form>
    <p class="text-muted">Estimated Time:</p>
        <div class="form-group mb-2">
          <label class="text-muted d-block" for="completedTime">Completed time:</label>
          <input type="hidden" name ="completedTime" value = "Now()">
          <button type="submit" class="btn btn-primary">Order is Ready!</button>
        </div>
  </div>
</div>

